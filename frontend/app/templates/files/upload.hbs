<div class="panel panel-default">
  <div class="panel-body">
    <div class="form-group">
      <label for="uploadFile">File</label>
      {{view "upload-file" valueBinding="name" content=content id="uploadFile"}}
      <p class="help-block">Choose a stromx file.</p>
    </div>
    <div class="form-group">
      <button {{action 'cancel'}} type="submit" class="btn btn-default">Cancel</button>
      <button {{action 'upload'}} type="submit" class="btn btn-primary">Upload</button>
    </div>
  </div>
</div>
