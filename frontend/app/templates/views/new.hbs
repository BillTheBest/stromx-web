<ol class="breadcrumb">
  <li>{{#link-to "stream.index"}}{{model.file.name}}{{/link-to}}</li>
  <li>New view</li>
</ol>

<h3>
  New view
</h3>

<div class="form-group">
  <label for="name">Name</label>
  {{input value=name class="form-control" placeholder="Enter view name" id="name"}}
</div>
<div class="form-group">
  <button {{action "cancel"}} type="submit" class="btn btn-default">Cancel</button>
  <button {{action "save"}} type="submit" class="btn btn-primary">Save</button>
</div>
