{{#if isList}}
  {{#each listData key="@index" as |item|}}
    {{svg-value value=item observer=observer}}
  {{/each}}
{{else}}
  {{#if svgImage}}
    <image width={{imageWidth}} height={{imageHeight}}
    xlink:href={{imageData}} />
  {{/if}}
  {{#if svgText}}
    <text x="10" y="20" fill={{color}}>{{textData}}</text>
  {{/if}}
  {{#if svgLines}}
    {{#each matrixDataNx4 key="@index" as |line|}}
      {{x-svg-line model=line color=color}}
    {{/each}}
  {{/if}}
  {{#if svgPoints}}
    {{#each matrixDataNx2 key="@index" as |point|}}
      {{x-svg-point model=point color=color}}
    {{/each}}
  {{/if}}
  {{#if svgPolygon}}
    <polygon points={{pointsData}} stroke={{color}} fill="none"/>
  {{/if}}
  {{#if svgPolyline}}
    <polyline points={{pointsData}} stroke={{color}} fill="none"/>
  {{/if}}
  {{#if svgRectangle}}
    {{#each matrixDataNx4 key="@index" as |rect|}}
      {{x-svg-rectangle model=rect color=color}}
    {{/each}}
  {{/if}}
  {{#if svgRotatedRectangle}}
    {{#each matrixDataNx5 key="@index" as |rect|}}
      {{x-svg-rotated-rectangle model=rect color=color}}
    {{/each}}
  {{/if}}
{{/if}}
