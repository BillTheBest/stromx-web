{{#if isList}}
  {{#each item in listData}}
    {{svg-value value=item observer=observer}}
  {{/each}}
{{else}}
  {{#if svgImage}}
    <image width={{imageWidth}} height={{imageHeight}}
    xlink:href={{imageData}} />
  {{/if}}
  {{#if svgText}}
    <text x="10" y="20" fill={{color}}>{{textData}}</text>
  {{/if}}
  {{#if svgLines}}
    {{#each line in matrixDataNx4 itemController="view.line"}}
      <line x1={{line.x1}} y1={{line.y1}} x2={{line.x2}} y2={{line.y2}} stroke={{color}} />
    {{/each}}
  {{/if}}
  {{#if svgPoints}}
    {{#each point in matrixDataNx2 itemController="view.circle"}}
      <circle cx={{point.cx}} cy={{point.cy}} stroke={{color}} r="3"/>
    {{/each}}
  {{/if}}
  {{#if svgPolygon}}
    <polygon points={{pointsData}} stroke={{color}} fill="none"/>
  {{/if}}
  {{#if svgPolyline}}
    <polyline points={{pointsData}} stroke={{color}} fill="none"/>
  {{/if}}
  {{#if svgRectangle}}
    {{#each rect in matrixDataNx4 itemController="view.rectangle"}}
      <rect x={{rect.x}} y={{rect.y}} width={{rect.width}} height={{rect.height}} stroke={{color}} fill="none"/>
    {{/each}}
  {{/if}}
  {{#if svgRotatedRectangle}}
    {{#each rect in matrixDataNx5 itemController="view.rotatedRectangle"}}
      {{#svg-transform transform=rect.transform}}
        <rect x={{rect.x}} y={{rect.y}} width={{rect.width}} height={{rect.height}} stroke={{color}} fill="none"/>
      {{/svg-transform}}
    {{/each}}
  {{/if}}
{{/if}}
