 <div class="panel panel-default">
  <div class="panel-body">
    <div class="form-group">
      {{view Ember.Select value=operator content=stream.operators optionValuePath="content"
         optionLabelPath="content.name" prompt="Select an operator"}}
      {{view Ember.Select value=connector content=connectors optionValuePath="content"
         optionLabelPath="content.title"prompt="Select a connector"}}
    </div>
    <button {{action 'saveObserver'}} type="submit" class="btn btn-default">Save</button>
  </div>
</div>
